<div class="col-md-1">
  <%= link_to 'Назад', all_types_path %>
</div>
<div class="col-md-8">
  <%if @torre_travel.images.any?%>
    <div class="frame">
      <div class="fotorama" data-width="768" data-height="576" data-loop="true" data-autoplay="3000">
        <% @torre_travel.images.each do |image|%>
          <%= image_tag image.image_of_housing.url(:large) %>
        <%end%>
      </div>
    </div>  
  <%else%>
     <%=image_tag("large/missing2.png")%>
  <%end%> 
  
  <div id="popup_name" class="popup_block">
    <%=render partial:"partials/left"%>
  </div> 
</div> 
